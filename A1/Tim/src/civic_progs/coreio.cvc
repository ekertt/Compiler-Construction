#include "core.cvc"

void fibs(int n) {
    if (n <= 1) {
        printInt(fib(n));

        printNewlines(1);

        return;
    }

    printInt(fib(n));

    printSpaces(1);

    fibs(n - 1);
}

void primes(int n) {
    if (n <= 1) {
        printNewlines(1);

        return;
    }

    if (isprime(n)) {
        printInt(n);
        
        printSpaces(1);

        primes(n - 1);
    } else {
        primes(n - 1);

        return;
    }
}

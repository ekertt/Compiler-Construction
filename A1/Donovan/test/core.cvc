int gcd (int a, int b) {
    if (a > b) {
        return a;
    } else {
        return b;
    }
}

/**
* yes
*/
int fac (int n) {
    int factorial;
    for (i = 1; i <= n; i++) {
        factorial = factorial * i;
    }

    return factorial;
}

/**
* yes
*/
int fib (int n) {
    if (n <= 1) {
        return n;
    }
    return fib(n-1) + fib(n-2);
}

/**
* yes
*/
bool isprime (int n) {
    if (n / n == 1 && n / 1 == n){
        return true;
    } else {
        return false;
    }
}